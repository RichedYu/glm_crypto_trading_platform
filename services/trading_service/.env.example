# Bybit API Configuration
BYBIT_API_KEY=your_api_key_here
BYBIT_API_SECRET=your_api_secret_here
BYBIT_TESTNET=true

# Service Endpoints
STRATEGY_SERVICE_ENDPOINTS=["http://localhost:8001"]
SENTIMENT_SERVICE_ENDPOINTS=["http://localhost:8002"]

# Strategy Parameters - S1
S1_SYMBOLS=["BTCUSDT", "ETHUSDT"]
S1_TIMEFRAME=1h
S1_RISK_PERCENT=2.0
S1_MAX_POSITION_SIZE=1000
S1_STOP_LOSS_PERCENT=2.0
S1_TAKE_PROFIT_PERCENT=4.0

# API Proxy Settings
API_PROXY_FAIL_THRESHOLD=3
API_PROXY_COOLDOWN_SECONDS=300
API_PROXY_TIMEOUT=10
